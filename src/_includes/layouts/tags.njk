---
permalink: /{{ locale }}/tags/
layout: layouts/base.njk
---

{% Heading {
  title: 'Tags'
} %}

{% GridList {
  className: 'tags--archive'
} %}

{% for tag in collections.tagList %}
  {% set tagUrl %}/tags/{{ tag }}/{% endset %}
  <a href="{{ tagUrl | url }}" class="tag">{{ tag }}</a>
{% endfor %}

{% endGridList %}
