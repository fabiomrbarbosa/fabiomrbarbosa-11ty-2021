<nav class="menu">
  <ul class="menu__list">
    {%- for entry in collections[locale] | eleventyNavigation -%}

    <li class="menu__item"{% if page.url == entry.url %} aria-current="page"{% endif %}>
      <a href="{{ entry.url | url }}">{{ entry.title }}</a>
    </li>

    {%- endfor -%}
  </ul>
</nav>
