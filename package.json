{
  "name": "fabiomrbarbosa",
  "version": "2.0.0",
  "description": "Fábio M.R. Barbosa's official website, built with Eleventy, hosted on Netlify",
  "scripts": {
    "sass": "node-sass src/assets/styles/main.scss src/_includes/styles/main.min.css --output-style compressed",
    "build": "npm run sass && npx eleventy && workbox generateSW workbox-config.js",
    "develop": "concurrently \"npm run sass -- --watch\" \"npx eleventy --serve\"",
    "serve": "eleventy --serve",
    "debug": "DEBUG=* eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fabiomrbarbosa/fabiomrbarbosa.git"
  },
  "author": {
    "name": "Fábio M.R. Barbosa",
    "email": "hello@fabiomrbarbosa.com",
    "url": "https://fabiomrbarbosa.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabiomrbarbosa/fabiomrbarbosa/issues"
  },
  "homepage": "https://github.com/fabiomrbarbosa/fabiomrbarbosa#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "concurrently": "^5.3.0",
    "eleventy-plugin-embed-everything": "^1.10.0",
    "image-size": "^0.9.3",
    "luxon": "^1.25.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.0",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-external-links": "0.0.6",
    "node-sass": "^5.0.0",
    "purgecss": "^4.0.0",
    "workbox-cli": "^6.0.2"
  }
}
